#include<stdio.h>
main()
{
	int n;
	printf("enter the marks secured:");
	scanf("%d",&n);
	if(n>=85&&n<=100)
	{
		printf("grade A");
	}
	else if(n>=70&&n<85)
	{
		printf("grade B");
	}
	else if(n>=55&&n<=69)
	{
		printf("grade C");
	}
	else if(n>=40&&n<=54)
	{
		printf("grade D");
	}
	else if(n>=0&&n<40)
	{
		printf("grade F");
	}
	else 
	{
		printf("INVALID MARKS");
		
	}
	return 0;
}
